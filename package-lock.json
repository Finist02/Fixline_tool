{
	"name": "fixline-tool",
	"displayName": "Fixline tool",
	"description": "Fixline tool",
	"version": "1.1.2",
	"publisher": "Danil",
	"icon": "images/fixline.png",
	"repository": {
		"url": "https://github.com/Finist02/Fixline_tool",
		"type": "git"
	},
	"engines": {
		"vscode": "^1.71.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:ctrl",
		"onLanguage:ctrlpp"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.OpenUnitTest",
				"title": "Open UnitTest",
				"icon": "$(preview)",
				"category": "Fixline"
			},
			{
				"command": "extension.CreateChildClass",
				"title": "Create child class",
				"icon": "$(preview)",
				"category": "Fixline"
			},
			{
				"command": "extension.OpenPanel",
				"title": "Open panel",
				"icon": "$(preview)",
				"category": "Fixline"
			},
			{
				"command": "extension.OpenHelpCtrl",
				"title": "Open ctrl Help",
				"icon": "$(book)",
				"category": "Fixline"
			},
			{
				"command": "extension.OpenProjectPanel",
				"title": "Open project panel",
				"icon": "$(preview)",
				"category": "Fixline"
			},
			{
				"command": "extension.CheckScript",
				"title": "Check Script",
				"icon": "$(debug)",
				"category": "Fixline"
			},
			{
				"command": "extension.CreateChangelog",
				"title": "Create changelog",
				"icon": "$(debug)",
				"category": "Fixline"
			},
			{
				"command": "extension.CreateDoxyHelp",
				"title": "Create help doxygen",
				"icon": "$(debug)",
				"category": "Fixline"
			},
			{
				"command": "extension.GetHelpChatGpt",
				"title": "Get help Ctrlpp",
				"icon": "$(debug)",
				"category": "Fixline"
			},
			{
				"command": "extension.RunScript",
				"title": "Run script",
				"icon": "$(run)",
				"category": "Fixline"
			},
			{
				"command": "extension.OpenLogs",
				"title": "Open Log",
				"icon": "$(list-flat)",
				"category": "Fixline"
			},
			{
				"command": "extension.Panelpreview",
				"title": "Panel preview",
				"icon": "$(open-preview)",
				"category": "Fixline"
			},
			{
				"command": "extension.StartUnitTests",
				"title": "Start tests",
				"category": "Fixline"
			},
			{
				"command": "extension.CreateUMLDiagrams",
				"title": "Create UML diagrams",
				"category": "Fixline"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.OpenPanel",
					"when": "resourceExtname == .xml",
					"group": "navigation@6"
				},
				{
					"command": "extension.Panelpreview",
					"when": "resourceExtname == .xml",
					"group": "navigation@6"
				},
				{
					"command": "extension.CheckScript",
					"when": "resourceExtname == .ctl",
					"group": "navigation@6"
				},
				{
					"command": "extension.RunScript",
					"when": "resourceExtname == .ctl",
					"group": "navigation@6"
				}
			],
			"editor/title": [
				{
					"command": "extension.OpenPanel",
					"group": "navigation",
					"when": "resourceExtname == .xml"
				},
				{
					"command": "extension.Panelpreview",
					"group": "navigation",
					"when": "resourceExtname == .xml"
				},
				{
					"command": "extension.CheckScript",
					"group": "navigation",
					"when": "resourceExtname == .ctl"
				},
				{
					"command": "extension.RunScript",
					"group": "navigation",
					"when": "resourceExtname == .ctl"
				}
			]
		},
		"configuration": [
			{
				"type": "object",
				"title": "Fixline tool",
				"properties": {
					"FixLineTool.Syntax.CheckSyntax": {
						"type": "boolean",
						"default": false,
						"description": "Ð¡heck syntax"
					},
					"FixLineTool.Syntax.InnersReadFiles": {
						"type": "number",
						"default": 2,
						"description": "Deep read files"
					},
					"FixLineTool.Syntax.HighlightCommentDoxygen": {
						"type": "boolean",
						"default": false,
						"description": "Highlight comment Doxygen"
					},
					"FixLineTool.OpenPanel.UserName": {
						"type": "string",
						"default": "root",
						"description": "User Name"
					},
					"FixLineTool.OpenPanel.Dollars": {
						"type": "string",
						"default": "",
						"description": "Dollars"
					},
					"FixLineTool.OpenPanel.Password": {
						"type": "string",
						"default": "",
						"description": "Password"
					},
					"FixLineTool.Gitlab.GitlabUrl": {
						"type": "string",
						"default": "http:///git.fixline.tech/",
						"description": "GitlabUrl"
					}
				}
			}
		]
	},
	"capabilities": {
		"definitionProvider": "true"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@types/vscode": "^1.71.0",
		"@typescript-eslint/eslint-plugin": "^5.31.0",
		"@typescript-eslint/parser": "^5.31.0",
		"@vscode/test-electron": "^2.1.5",
		"eslint": "^8.20.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"axios": "^1.3.1",
		"braces": "^3.0.3",
		"fsp-xml-parser": "^1.0.0",
		"highlight.js": "^11.8.0",
		"markdown-it": "^13.0.1",
		"xml2js": "^0.6.2"
	},
	"__metadata": {
		"id": "7f028623-2bae-4972-9f01-779c42a804b4",
		"publisherId": "e900b8a2-658b-4c43-96ce-dda5da16b93a",
		"publisherDisplayName": "Danil",
		"targetPlatform": "undefined",
		"isApplicationScoped": false,
		"isPreReleaseVersion": false,
		"hasPreReleaseVersion": false,
		"installedTimestamp": 1723773736709,
		"pinned": false,
		"preRelease": false,
		"source": "gallery"
	}
}
